datasource db {
  // url      = env("DATABASE_URL")
  url      = "postgresql://postgres:postgres@db:5432/postgres"
  provider = "postgresql"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  email     String   @unique
  name      String?
}